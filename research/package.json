{
  "name": "open-ui-docs",
  "description": "Open UI documentation.",
  "version": "1.0.0",
  "author": "Open UI",
  "repository": {
    "type": "git",
    "url": "https://github.com/WICG/open-ui"
  },
  "keywords": [
    "open ui"
  ],
  "license": "MIT",
  "scripts": {
    "build": "gatsby build",
    "clean": "gatsby clean",
    "serve": "gatsby serve",
    "start": "gatsby develop"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "post-commit": "git update-index --again"
    }
  },
  "lint-staged": {
    "**/*.js": [
      "prettier --write",
      "git add"
    ],
    "**/*.{json,json5}": [
      "prettier --parser json --write",
      "git add"
    ],
    "**/*.md": [
      "prettier --parser markdown --write",
      "git add"
    ],
    "**/*.mdx": [
      "prettier --parser mdx --write",
      "git add"
    ]
  },
  "dependencies": {
    "@emotion/core": "^10.0.28",
    "@fortawesome/fontawesome-svg-core": "^1.2.28",
    "@fortawesome/free-brands-svg-icons": "^5.13.0",
    "@fortawesome/free-solid-svg-icons": "^5.15.1",
    "@fortawesome/react-fontawesome": "^0.1.9",
    "@mdx-js/mdx": "^1.5.8",
    "@mdx-js/react": "^1.5.8",
    "anchor-js": "^4.2.2",
    "gatsby": "^4.25.7",
    "gatsby-plugin-favicon": "^3.1.6",
    "gatsby-plugin-fontawesome-css": "^1.0.0",
    "gatsby-plugin-mdx": "^1.1.6",
    "gatsby-plugin-offline": "^3.1.2",
    "gatsby-plugin-react-helmet": "^3.2.1",
    "gatsby-plugin-remove-trailing-slashes": "^2.2.1",
    "gatsby-plugin-sharp": "2.5.4",
    "gatsby-plugin-theme-ui": "^0.3.0",
    "gatsby-plugin-typography": "^2.4.1",
    "gatsby-source-filesystem": "^2.2.2",
    "husky": "^4.2.3",
    "json5-loader": "^3.0.0",
    "lint-staged": "^10.1.2",
    "lodash": "^4.17.15",
    "prettier": "^2.0.4",
    "prism-react-renderer": "^1.1.1",
    "prismjs": "^1.22.0",
    "prop-types": "^15.7.2",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-helmet": "^5.2.1",
    "react-typography": "^0.16.19",
    "remark-emoji": "^2.1.0",
    "remark-images": "^2.0.0",
    "sharp": "^0.25.2",
    "theme-ui": "^0.3.1",
    "typography": "^0.16.19"
  }
}
